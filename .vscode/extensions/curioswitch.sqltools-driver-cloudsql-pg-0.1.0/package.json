		"@types/vscode": "^1.42.0",
		"@vscode/vsce": "^2.22.0",
		"esbuild": "^0.19.10",
		"prettier": "^3.1.1",
		"rimraf": "^5.0.5",
		"typescript": "^5.3.3"
	},
	"packageManager": "yarn@4.0.2",
